{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Webpack Boilterplate",
  "repository": "https://github.com/mariobrusarosco/webpack-boilerplate.git",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config=config/webpack.dev.js",
    "dev": "cross-env development=true APP_NAME=WEBPACK_BOILERPALTE nodemon --inspect --watch config --watch server server/main.js",
    "build": "cross-env production=true APP_NAME=WEBPACK_BOILERPALTE webpack --config=config/webpack.prod.js",
    "build:dev": "cross-env development=true APP_NAME=WEBPACK_BOILERPALTE webpack --config=config/webpack.dev.js",
    "prod": "yarn build && cross-env production=true node server/main.js",
    "server": "serve --listen 8080 dist",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "e2e:dev": "start-server-and-test dev http-get://localhost:8080 cy:open",
    "e2e:prod": "start-server-and-test server :8080 && cy:run"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-minify": "^0.4.3",
    "babel-minify-webpack-plugin": "^0.3.1",
    "brotli-webpack-plugin": "^1.0.0",
    "classnames": "^2.2.6",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "cypress": "^3.1.4",
    "express": "^4.16.3",
    "express-static-gzip": "^1.1.3",
    "extract-loader": "^3.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "markdown-loader": "^4.0.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-hot-loader": "^4.3.8",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.24.0"
  },
  "devDependencies": {
    "serve": "^10.1.1",
    "start-server-and-test": "^1.7.11"
  }
}
